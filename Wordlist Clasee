	@Override
	public ListIterator<Word> listIterator() {
		// TODO Auto-generated method stub
		return new WordListIterator(0);
	}

	class WordListIterator implements ListIterator<Word> {
		int nextToReturn = 0;
		public WordListIterator(int index){
		nextToReturn = index;
		}
		
		@Override
		public void add(Word arg0) {
			throw new UnsupportedOperationException("Not allowed");
		}

		@Override
		public boolean hasNext() {
			
			
			return nextToReturn<size();
		}

		@Override
		public boolean hasPrevious() {
			// TODO Auto-generated method stub
			
			return nextToReturn>0;
		}

