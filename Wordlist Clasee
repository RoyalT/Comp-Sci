import java.util.Collection;
import java.util.Iterator;
import java.util.List;
import java.util.ListIterator;


public class WordList implements List<Word> {
	int size = 0;
	int capacity = 10;
	Word[] word = new Word[capacity];
	
	public int binSearch(Word e, int min, int max)
	{
		int mid =-1;
		
		if(max<=min)
			return -mid;
		
		mid = (min+max)/2;
		
		if(word[mid].compareTo(e)>0)
			return binSearch(e,mid+1,max);
		else if (word[mid].compareTo(e)<0)
			return binSearch(e,min,mid-1);
		else 
			return mid;
		
	}
	
	
	
	@Override
	public boolean add(Word e) {
		// TODO Auto-generated method stub
		if(indexOf(e)!<0)
		{
			indexOf(e).increase();
			return true;
		}
		if(indexOf(e)<size())
		
		
		return false;
	}

